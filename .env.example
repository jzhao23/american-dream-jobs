# CareerOneStop API (optional - data will be generated from seeds if not set)
# Register at: https://www.careeronestop.org/Developers/WebAPI/registration.aspx
COS_API_KEY=your_api_key_here
COS_USER_ID=your_user_id_here

# Analytics (optional)
# Plausible - privacy-friendly analytics (recommended)
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=your-domain.com

# Google Analytics 4 (optional)
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Email service (for future integration)
# CONVERTKIT_API_KEY=your_key_here
# CONVERTKIT_FORM_ID=your_form_id

# Reddit API (for fetching career testimonials)
# Register at: https://www.reddit.com/prefs/apps
# Create a "script" type application
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# Anthropic API (for AI enrichment of reviews and Career Compass)
# Get your key at: https://console.anthropic.com
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenAI API (for embeddings in Career Compass)
# Get your key at: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key

# Supabase (for vector storage in Career Compass)
# Get your credentials at: https://supabase.com/dashboard
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_KEY=your_service_key

# Job Search APIs (for Find Jobs feature)
# SerpApi - aggregates jobs from Google Jobs (LinkedIn, Indeed, Glassdoor)
# Get your key at: https://serpapi.com (250 free searches/month)
SERPAPI_API_KEY=your_serpapi_key

# JSearch (RapidAPI) - fallback job search API
# Get your key at: https://rapidapi.com/letscrape-6bRBa3QguO5/api/jsearch
# (500 free searches/month)
JSEARCH_API_KEY=your_rapidapi_key
